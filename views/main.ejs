      <div class="nav px-4 py-4 flex justify-between">
        <i class="fa-solid fa-bars"></i>
        <div class="links flex gap-6">
          <a href="">Login</a> <a href="">Register</a>
        </div>
      </div>
      <form action="" class="flex mt-4 justify-center">
        <input
          class="px-4 py-2 rounded-l-md text-black outline-none"
          type="text"
          placeholder="Search places"
          name="search-bar"
        />
        <button class="bg-gray-500 rounded-r-md px-4">
          <i class="fa-solid fa-magnifying-glass"></i>
        </button>
      </form>
      <div id="map" style="width: 400px; height: 300px"></div>
    </div>
    <script src="/js/mapbox.js" defer></script>
